{
  "name": "cnn-logger",
  "version": "2.1.0",
  "description": "CNN Logger",
  "main": "lib/logger.js",
  "directories": {
    "example": "./example",
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint . && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-logger.git"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-logger/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-logger",
  "engines": {
    "node": ">=10.12.0"
  },
  "dependencies": {
    "nconf": "0.11.0",
    "winston": "3.2.1",
    "winston-logzio": "5.0.4"
  },
  "devDependencies": {
    "acorn": "7.2.0",
    "changelog-maker": "2.4.0",
    "esdoc": "1.1.0",
    "eslint": "7.0.0",
    "jsonlint": "1.6.3"
  },
  "keywords": [
    "cnn",
    "cnnlabs",
    "logger",
    "logstash",
    "logz.io",
    "winston"
  ],
  "license": "Apache-2.0",
  "resolutions": {
    "**/**/marked": "0.8.2",
    "**/**/minimist": "1.2.5"
  }
}
