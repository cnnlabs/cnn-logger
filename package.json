{
  "name": "cnn-logger",
  "version": "2.0.1",
  "description": "CNN Logger",
  "main": "lib/logger.js",
  "directories": {
    "example": "./example",
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint . && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-logger.git"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-logger/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-logger",
  "engines": {
    "node": ">=8.12.0"
  },
  "dependencies": {
    "nconf": "0.10.0",
    "winston": "3.2.1",
    "winston-logzio": "4.0.1"
  },
  "devDependencies": {
    "changelog-maker": "2.3.0",
    "esdoc": "1.1.0",
    "eslint": "5.13.0",
    "jsonlint": "1.6.3"
  },
  "keywords": [
    "cnn",
    "cnnlabs",
    "logger",
    "logstash",
    "logz.io",
    "winston"
  ],
  "license": "Apache-2.0"
}
