{
  "name": "cnn-logger",
  "version": "1.0.0",
  "description": "CNN Logger",
  "main": "lib/logger.js",
  "directories": {
    "example": "./example",
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint . && test/validate-json-files.sh",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-logger.git"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-logger/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-logger",
  "engines": {
    "node": ">=4.4.7"
  },
  "dependencies": {
    "nconf": "0.8.4",
    "winston": "2.2.0",
    "winston-logzio": "1.0.0"
  },
  "devDependencies": {
    "changelog-maker": "2.2.3",
    "esdoc": "0.4.7",
    "eslint": "2.13.1",
    "jsonlint": "1.6.2",
    "npm-check-updates": "2.6.8"
  },
  "keywords": [
    "cnn",
    "cnnlabs",
    "logger",
    "logstash",
    "logz.io",
    "winston"
  ],
  "license": "Apache-2.0"
}
